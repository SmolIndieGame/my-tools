{
  "name": "my-tools",
  "displayName": "my-tools",
  "description": "Just some scripts to make my vscode experience a little bit smoother.",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/SmolIndieGame/my-tools.git"
  },
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "keybindings": [
      {
        "key": "alt+`",
        "command": "workbench.view.extension.tabManagement"
      },
      {
        "key": "alt+`",
        "command": "workbench.action.closeSidebar",
        "when": "sideBarFocus && activeViewlet == 'workbench.view.extension.tabManagement'"
      },
      {
        "key": "d",
        "command": "my-tools.tabManagement.closeEditor",
        "when": "sideBarFocus && activeViewlet == 'workbench.view.extension.tabManagement'",
        "args": 0
      },
      {
        "key": "alt+k",
        "command": "my-tools.tabManagement.moveTabUp",
        "when": "sideBarFocus && activeViewlet == 'workbench.view.extension.tabManagement'"
      },
      {
        "key": "alt+j",
        "command": "my-tools.tabManagement.moveTabDown",
        "when": "sideBarFocus && activeViewlet == 'workbench.view.extension.tabManagement'"
      },
      {
        "key": "1",
        "command": "my-tools.tabManagement.focusAtIndex",
        "when": "sideBarFocus && activeViewlet == 'workbench.view.extension.tabManagement'",
        "args": 1
      },
      {
        "key": "2",
        "command": "my-tools.tabManagement.focusAtIndex",
        "when": "sideBarFocus && activeViewlet == 'workbench.view.extension.tabManagement'",
        "args": 2
      },
      {
        "key": "3",
        "command": "my-tools.tabManagement.focusAtIndex",
        "when": "sideBarFocus && activeViewlet == 'workbench.view.extension.tabManagement'",
        "args": 3
      },
      {
        "key": "4",
        "command": "my-tools.tabManagement.focusAtIndex",
        "when": "sideBarFocus && activeViewlet == 'workbench.view.extension.tabManagement'",
        "args": 4
      },
      {
        "key": "5",
        "command": "my-tools.tabManagement.focusAtIndex",
        "when": "sideBarFocus && activeViewlet == 'workbench.view.extension.tabManagement'",
        "args": 5
      },
      {
        "key": "6",
        "command": "my-tools.tabManagement.focusAtIndex",
        "when": "sideBarFocus && activeViewlet == 'workbench.view.extension.tabManagement'",
        "args": 6
      },
      {
        "key": "7",
        "command": "my-tools.tabManagement.focusAtIndex",
        "when": "sideBarFocus && activeViewlet == 'workbench.view.extension.tabManagement'",
        "args": 7
      },
      {
        "key": "8",
        "command": "my-tools.tabManagement.focusAtIndex",
        "when": "sideBarFocus && activeViewlet == 'workbench.view.extension.tabManagement'",
        "args": 8
      },
      {
        "key": "9",
        "command": "my-tools.tabManagement.focusAtIndex",
        "when": "sideBarFocus && activeViewlet == 'workbench.view.extension.tabManagement'",
        "args": 9
      },
      {
        "key": "0",
        "command": "my-tools.tabManagement.focusAtIndex",
        "when": "sideBarFocus && activeViewlet == 'workbench.view.extension.tabManagement'",
        "args": 0
      },
      {
        "key": "alt+1",
        "command": "my-tools.tabManagement.openAtIndex",
        "when": "activeViewlet == 'workbench.view.extension.tabManagement'",
        "args": 1
      },
      {
        "key": "alt+2",
        "command": "my-tools.tabManagement.openAtIndex",
        "when": "activeViewlet == 'workbench.view.extension.tabManagement'",
        "args": 2
      },
      {
        "key": "alt+3",
        "command": "my-tools.tabManagement.openAtIndex",
        "when": "activeViewlet == 'workbench.view.extension.tabManagement'",
        "args": 3
      },
      {
        "key": "alt+4",
        "command": "my-tools.tabManagement.openAtIndex",
        "when": "activeViewlet == 'workbench.view.extension.tabManagement'",
        "args": 4
      },
      {
        "key": "alt+5",
        "command": "my-tools.tabManagement.openAtIndex",
        "when": "activeViewlet == 'workbench.view.extension.tabManagement'",
        "args": 5
      },
      {
        "key": "alt+6",
        "command": "my-tools.tabManagement.openAtIndex",
        "when": "activeViewlet == 'workbench.view.extension.tabManagement'",
        "args": 6
      },
      {
        "key": "alt+7",
        "command": "my-tools.tabManagement.openAtIndex",
        "when": "activeViewlet == 'workbench.view.extension.tabManagement'",
        "args": 7
      },
      {
        "key": "alt+8",
        "command": "my-tools.tabManagement.openAtIndex",
        "when": "activeViewlet == 'workbench.view.extension.tabManagement'",
        "args": 8
      },
      {
        "key": "alt+9",
        "command": "my-tools.tabManagement.openAtIndex",
        "when": "activeViewlet == 'workbench.view.extension.tabManagement'",
        "args": 9
      },
      {
        "key": "alt+0",
        "command": "my-tools.tabManagement.openAtIndex",
        "when": "activeViewlet == 'workbench.view.extension.tabManagement'",
        "args": 0
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "tabManagement",
          "title": "Tab Management",
          "icon": "media/dep.svg"
        }
      ]
    },
    "views": {
      "tabManagement": [
        {
          "id": "tabManagement",
          "name": "Tab Management"
        }
      ]
    },
    "commands": [
      {
        "command": "my-tools.tabManagement.moveTabUp",
        "title": "Move Tab Left",
        "category": "View"
      },
      {
        "command": "my-tools.tabManagement.moveTabDown",
        "title": "Move Tab Right",
        "category": "View"
      },
      {
        "command": "my-tools.tabManagement.focusAtIndex",
        "title": "my-tools.tabManagement.focusAtIndex"
      },
      {
        "command": "my-tools.tabManagement.openAtIndex",
        "title": "my-tools.tabManagement.openAtIndex"
      },
      {
        "command": "my-tools.tabManagement.closeEditor",
        "title": "my-tools.tabManagement.closeEditor"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.76.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.2.3"
  }
}